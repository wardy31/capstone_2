{"ast":null,"code":"import { mapState } from \"vuex\";\nimport EditProfile from \"@/views/admin/Dialog/Profile/EditProfile.vue\";\nexport default {\n  components: {\n    EditProfile\n  },\n  mounted() {\n    console.log(\"user\", this.user);\n  },\n  data() {\n    return {\n      old_password: \"\",\n      new_password: \"\",\n      new_password_confirmation: \"\",\n      profile: \"\",\n      snackbar: false\n    };\n  },\n  computed: {\n    ...mapState({\n      loading: state => state.clinic.credentials.loading,\n      user: state => state.clinic.credentials.data,\n      success: state => state.clinic.credentials.success,\n      error: state => state.clinic.credentials.error\n    })\n  },\n  methods: {\n    handlePassword() {\n      const forms = {\n        old_password: this.old_password,\n        new_password: this.new_password,\n        new_password_confirmation: this.new_password_confirmation\n      };\n      this.$store.dispatch(\"clinic/changePassword\", {\n        id: this.user.id,\n        forms: forms\n      });\n      console.log(forms + this.user.id);\n    },\n    editDialog() {\n      this.profile = this.user;\n    }\n  }\n};","map":{"version":3,"mappings":"AAiIA;AACA;AACA;EACAA;IACAC;EACA;EACAC;IACAC;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACAC;MACA;QACAX;QACAC;QACAC;MACA;MACA;QAAAU;QAAAC;MAAA;MACAf;IACA;IACAgB;MACA;IACA;EACA;AACA","names":["components","EditProfile","mounted","console","data","old_password","new_password","new_password_confirmation","profile","snackbar","computed","loading","user","success","error","methods","handlePassword","id","forms","editDialog"],"sourceRoot":"src/views/admin/Profile","sources":["EditProfile.vue"],"sourcesContent":["<template>\r\n  <v-app>\r\n    <v-main class=\"bg\">\r\n      <v-container class=\"ml-10\">\r\n        <div class=\"mt-12\">\r\n          <h2 class=\"font-weight-bold\">Edit Personal Profile</h2>\r\n        </div>\r\n        <div class=\"my-8\">\r\n          <v-avatar color=\"primary\" size=\"90\" class=\"white--text\" tile\r\n            ><h3>{{ user.first_name.slice(0,1).toUpperCase() }}</h3></v-avatar\r\n          >\r\n        </div>\r\n\r\n        <v-divider></v-divider>\r\n        <div class=\"mb-2 mt-5\">\r\n          <h3 class=\"gray--text mb-2\">Personal Information</h3>\r\n          <v-btn color=\"primary\" outlined small class=\"mb-5\" @click=\"editDialog\"\r\n            ><v-icon left>edit</v-icon> Edit Profile</v-btn\r\n          >\r\n        </div>\r\n\r\n        <div class=\"d-flex mb-2\">\r\n          <h4>Name:</h4>\r\n          <h4 class=\"ml-2 font-weight-bold primary--text text-capitalize\">\r\n            {{ `${user.first_name} ${user.middle_name} ${user.last_name}` }}\r\n          </h4>\r\n        </div>\r\n        <!-- \r\n            <div class=\"d-flex mb-3\">\r\n                <h4>Age:</h4>\r\n                <h4 class=\"ml-2 font-weight-bold primary--text\">21</h4>\r\n            </div> -->\r\n\r\n        <div class=\"d-flex mb-3\">\r\n          <h4>Gender:</h4>\r\n          <h4 class=\"ml-2 font-weight-bold primary--text text-capitalize\">\r\n            {{ user.gender }}\r\n          </h4>\r\n        </div>\r\n\r\n        <div class=\"d-flex mb-3\">\r\n                <h4>Address:</h4>\r\n                <h4 class=\"ml-2 font-weight-bold primary--text\">{{user.address}}</h4>\r\n            </div>\r\n\r\n        <div class=\"d-flex mb-3\">\r\n          <h4>Contact Number:</h4>\r\n          <h4 class=\"ml-2 font-weight-bold primary--text\">\r\n            {{ user.contact_number }}\r\n          </h4>\r\n        </div>\r\n\r\n        <div class=\"d-flex mb-3\">\r\n          <h4>Email:</h4>\r\n          <h4 class=\"ml-2 font-weight-bold primary--text\">{{ user.email }}</h4>\r\n        </div>\r\n\r\n        <div class=\"d-flex mb-3\">\r\n          <h4>Role:</h4>\r\n          <h4 class=\"ml-2 font-weight-bold primary--text text-capitalize\">\r\n            {{ user.role.name }}\r\n          </h4>\r\n        </div>\r\n        <!-- <div class=\"d-flex mb-3\">\r\n                <h4>Vaccination Status:</h4>\r\n                <h4 class=\"ml-2 font-weight-bold primary--text\">Fully Vaccinated</h4>\r\n            </div> -->\r\n\r\n        <v-divider class=\"mt-8\"></v-divider>\r\n        <div class=\"mb-6 mt-6\">\r\n          <h3 class=\"gray--text mb-2\">Change Security Password</h3>\r\n        </div>\r\n        <div>\r\n          <v-card color=\"transparent\" flat width=\"320\">\r\n            <v-alert type=\"success\" :value=\"success\">\r\n              Change Password Successfully\r\n            </v-alert>\r\n            <v-text-field\r\n              outlined\r\n              label=\"Current Password\"\r\n              type=\"password\"\r\n              dense\r\n              filled\r\n              :error-messages=\"error?.old_password?.length ? error.old_password : false\"\r\n              v-model=\"old_password\"\r\n            ></v-text-field>\r\n            <v-text-field\r\n              outlined\r\n              label=\"New Password\"\r\n              type=\"password\"\r\n              dense\r\n              filled\r\n              :error-messages=\"error?.new_password?.length ? error.new_password : false\"\r\n              v-model=\"new_password\"\r\n            ></v-text-field>\r\n            <v-text-field\r\n              outlined\r\n              label=\"Confirm New Password\"\r\n              type=\"password\"\r\n              dense\r\n              filled\r\n              v-model=\"new_password_confirmation\"\r\n              :error=\"error?.new_password?.length ? true : false\"\r\n            ></v-text-field>\r\n\r\n            <div class=\"mt-n2 d-flex justify-end\">\r\n              <v-btn class=\"primary text-capitalize\" @click=\"handlePassword\"><h5>Change Password</h5></v-btn>\r\n            </div>\r\n          </v-card>\r\n        </div>\r\n        <v-divider class=\"mt-8\"></v-divider>\r\n      </v-container>\r\n      <EditProfile :profile=\"profile\" @handleProfile=\"profile = null\" @snackBar=\"snackbar = true\"/>\r\n    </v-main>\r\n    <v-snackbar\r\n      :timeout=\"2000\"\r\n      v-model=\"snackbar\"\r\n      bottom\r\n      right\r\n      color=\"success\"\r\n      elevation=\"24\"\r\n    >\r\n      Successfully Updated Profile.\r\n    </v-snackbar>\r\n  </v-app>\r\n\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from \"vuex\";\r\nimport EditProfile from \"@/views/admin/Dialog/Profile/EditProfile.vue\"\r\nexport default {\r\n  components:{\r\n    EditProfile\r\n  },\r\n  mounted() {\r\n    console.log(\"user\", this.user);\r\n  },\r\n  data() {\r\n    return {\r\n      old_password: \"\",\r\n      new_password: \"\",\r\n      new_password_confirmation: \"\",\r\n      profile:\"\",\r\n      snackbar:false\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      loading: (state) => state.clinic.credentials.loading,\r\n      user: (state) => state.clinic.credentials.data,\r\n      success: (state) => state.clinic.credentials.success,\r\n      error: (state) => state.clinic.credentials.error,\r\n    }),\r\n  },\r\n  methods: {\r\n      handlePassword() {\r\n        const forms = {\r\n          old_password: this.old_password,\r\n          new_password: this.new_password,\r\n          new_password_confirmation: this.new_password_confirmation,\r\n        };\r\n        this.$store.dispatch(\"clinic/changePassword\", { id: this.user.id,forms:forms });\r\n        console.log(forms + this.user.id);\r\n      },\r\n      editDialog(){\r\n        this.profile = this.user\r\n      }\r\n    },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.bg {\r\n  background-color: #f8f0f0;\r\n}\r\n.adj {\r\n  width: 280px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}