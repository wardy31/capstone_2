\"\n                    filled\n                    dense\n                    outlined\n                    type=\"password\"\n                    v-model=\"clinic.password_confirmation\"\n                  ></v-text-field\n                ></v-col>\n  \n              </v-row>\n              \n          </div>\n            <v-card-actions\n              >\n              <v-spacer></v-spacer>\n              <v-btn text plain to=\"user-login\" class=\" text-capitalize\">Cancel</v-btn>\n              <v-btn color=\"primary\" @click=\"submit\" :loading=\"loading\" class=\"pa-5 px-12 text-capitalize\">Create Account</v-btn>\n              </v-card-actions>\n          </v-card>\n        </v-container>\n      </v-main>\n    </v-app>\n  </template>\n  \n  <script>\n  import { mapState } from 'vuex';\n  \n  export default {\n    computed:{\n      ...mapState({\n          loading: state => state.register.submit.loading,\n          error: state => state.register.submit.error,\n          message: state => state.register.submit.message\n      })\n    },\n    data() {\n      return {\n          clinic:{\n              first_name:\"\", \n              middle_name:\"\",\n              last_name:\"\",\n              deparment:\"\",\n              gender:\"\",\n              contact_number:\"\",\n              username:\"\",\n              password:\"\",\n              password_confirmation:\"\",\n              email:\"\",\n              vaccination_status:\"\",\n              classification_id: \"\"\n          }\n      \n      //   gender: [\n      //     { value: \"male\", label: \"Male\" },\n      //     { value: \"female\", label: \"Female\" },\n      //   ],\n      }\n    },\n    methods:{\n      submit(){\n          this.$store.dispatch('register/createUser',this.clinic)\n      }\n    }\n  };\n  </script>\n  "]},"metadata":{},"sourceType":"module","externalDependencies":[]}