{"ast":null,"code":"import { mapState } from \"vuex\";\nexport default {\n  props: ['dialog'],\n  computed: {\n    ...mapState({\n      loading: state => state.disease.store.loading,\n      error: state => state.disease.store.error,\n      message: state => state.staff.submit.message\n    })\n  },\n  data() {\n    return {\n      name: \"\"\n    };\n  },\n  methods: {\n    async submit() {\n      const res = await this.$store.dispatch(\"disease/store\", {\n        name: this.name\n      });\n      console.log(res);\n      if (res) {\n        this.name = \"\";\n        this.$emit('handleDialog');\n        this.$emit('snackBar');\n      }\n    },\n    async handleDialog() {\n      this.$emit('handleDialog');\n    }\n  }\n};","map":{"version":3,"mappings":"AA4CA;AAEA;EACAA;EACAC;IACA;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACA;MACA;QAAAD;MAAA;MACAE;MACA;QACA;QACA;QACA;MACA;IACA;IACA;MACA;IACA;EACA;AACA","names":["props","computed","loading","error","message","data","name","methods","console"],"sourceRoot":"src/views/admin/Dialog/Disease","sources":["DiseaseDialog.vue"],"sourcesContent":["<template>\r\n  <v-dialog :value=\"dialog\" fullscreen transition=\"dialog-bottom-transition\">\r\n    <v-card color=\"accent text-capitalize\">\r\n      <v-toolbar color=\"primary\">\r\n        <v-btn @click=\"handleDialog\" icon large color=\"accent\"\r\n          ><v-icon>close</v-icon></v-btn\r\n        >\r\n        <v-toolbar-title class=\"accent--text font-weight-bold\"\r\n          >Add Disease Classification</v-toolbar-title\r\n        >\r\n        <v-spacer></v-spacer>\r\n      </v-toolbar>\r\n      <v-container>\r\n        <h2 class=\"font-weight-regular mb-4 mt-4\">Disease Details</h2>\r\n\r\n        <v-row dense>\r\n          <v-col sm=\"5\">\r\n            <v-text-field\r\n              filled\r\n              outlined\r\n              :error-messages=\"error.name?.length ? error.name : false\"\r\n              label=\"Disease Name\"\r\n              