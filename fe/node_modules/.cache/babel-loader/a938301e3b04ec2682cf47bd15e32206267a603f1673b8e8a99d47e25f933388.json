;\n  },\n  methods: {\n    submit() {\n      this.$store.dispatch(\"station/submit\", this.station);\n    },\n    handleDialog() {\n      this.$store.state.station.submit.dialog = false;\n      this.$store.state.station.submit.error = false;\n    },\n    handleRequired(data){\n      console.log(data);\n    }\n  },\n};\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}