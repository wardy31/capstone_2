{"ast":null,"code":"import { mapState } from 'vuex';\nexport default {\n  mounted() {\n    this.$store.dispatch('clinic/getRole');\n  },\n  computed: {\n    ...mapState({\n      role: state => state.clinic.role,\n      loading: state => state.staff.submit.loading,\n      error: state => state.staff.submit.error,\n      message: state => state.staff.submit.message,\n      dialog: state => state.staff.submit.dialog\n    })\n  },\n  data() {\n    return {\n      clinic: {\n        first_name: \"\",\n        middle_name: \"\",\n        last_name: \"\",\n        gender: \"\",\n        role: null,\n        address: \"\",\n        contact_number: \"\",\n        email: \"\"\n      }\n    };\n  },\n  methods: {\n    submit() {\n      this.$store.dispatch('staff/createClinic', this.clinic);\n    },\n    handleDialog() {\n      this.$store.state.staff.submit.dialog = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAsFA;AAEA;EACAA;IACA;EACA;EACAC;IACA;MACAC;MACAC;MACAC;MACAC;MACAC;IACA;EACA;EACAC;IACA;MACAC;QACAC;QACAC;QACAC;QACAC;QACAV;QACAW;QACAC;QACAC;MACA;IACA;EACA;EACAC;IACAC;MACA;IACA;IACAC;MACA;IACA;EACA;AACA","names":["mounted","computed","role","loading","error","message","dialog","data","clinic","first_name","middle_name","last_name","gender","address","contact_number","email","methods","submit","handleDialog"],"sourceRoot":"src/views/admin/Dialog/Staff","sources":["AddDialog.vue"],"sourcesContent":["<template>\r\n  <v-dialog v-model=\"dialog\" fullscreen transition=\"dialog-bottom-transition\">\r\n    <v-card color=\"accent text-capitalize\">\r\n      <v-toolbar color=\"primary\"> \r\n        <v-btn @click=\"handleDialog\" icon large color=\"accent\"><v-icon>close</v-icon></v-btn>\r\n        <v-toolbar-title class=\"accent--text font-weight-bold\">Add Staff</v-toolbar-title>\r\n        <v-spacer></v-spacer>\r\n      </v-toolbar>\r\n      <v-container>\r\n        <h2 class=\"font-weight-regular mb-4 mt-4\">Personal Details</h2>\r\n        <v-alert\r\n          type=\"info\"\r\n          icon=\"\"\r\n          border=\"left\"\r\n          text\r\n          :value=\"true\"\r\n          class=\"mb-6\"\r\n        >\r\n          The Username and Password will automatically generate.\r\n        </v-alert>\r\n        \r\n        <v-alert type=\"error\" :value=\"error\">\r\n          {{message}}hahaha\r\n        </v-alert>\r\n        <v-row dense>\r\n          <v-col>\r\n            <v-text-field filled outlined label=\"First Name\" v-model=\"clinic.first_name\"></v-text-field>\r\n          </v-col>\r\n          <v-col>\r\n            <v-text-field filled outlined label=\"Middle Name\" v-model=\"clinic.middle_name\"></v-text-field>\r\n          </v-col>\r\n          <v-col>\r\n            <v-text-field filled outlined label=\"Last Name\" v-model=\"clinic.last_name\"></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n\r\n        <v-row dense class=\"mt-n3\">\r\n          <v-col cols=\"3\">\r\n            <v-select\r\n              outlined\r\n              filled\r\n              :items=\"['male', 'female']\"\r\n              v-model=\"clinic.gender\"\r\n              label=\"Gender\"\r\n            ></v-select>\r\n          </v-col>\r\n          <v-col cols=\"3\">\r\n            <v-select\r\n              outlined\r\n              filled\r\n              :items=\"role\"\r\n              item-text=\"name\"\r\n              item-value=\"id\"\r\n              v-model=\"clinic.role\"\r\n              label=\"Role\"\r\n            ></v-select>\r\n          </v-col>\r\n          <v-col cols=\"6\">\r\n            <v-text-field filled outlined label=\"Address\" v-model=\"clinic.address\"></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n        <v-row dense class=\"mt-n3\">\r\n          <v-col cols=\"4\">\r\n            <v-text-field filled outlined label=\"Contact Number\" v-model=\"clinic.contact_number\"></v-text-field>\r\n          </v-col>\r\n          <v-col cols=\"4\">\r\n            <v-text-field filled outlined label=\"Email\" v-model=\"clinic.email\"></v-text-field>\r\n          </v-col>\r\n        </v-row>\r\n        <v-row dense class=\"mt-n4\">\r\n          <v-col md=\"2\">\r\n            <v-btn\r\n              color=\"primary\"\r\n              block\r\n              :loading=\"loading\"\r\n              class=\"px-12 py-5 text-capitalize font-weight-bold\"\r\n              @click=\"submit()\">Add Staff</v-btn\r\n            >\r\n          </v-col>\r\n        </v-row>\r\n      </v-container>\r\n    </v-card>\r\n  </v-dialog>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex';\r\n\r\nexport default {\r\n  mounted(){\r\n  this.$store.dispatch('clinic/getRole')\r\n  },\r\n  computed:{\r\n    ...mapState({\r\n      role:state => state.clinic.role,\r\n      loading:state => state.staff.submit.loading,\r\n      error:state => state.staff.submit.error,\r\n      message:state => state.staff.submit.message,\r\n      dialog:state => state.staff.submit.dialog\r\n    })\r\n  },\r\n  data() {\r\n    return {\r\n      clinic:{\r\n            first_name:\"\", \r\n            middle_name:\"\",\r\n            last_name:\"\",\r\n            gender:\"\",\r\n            role:null,\r\n            address:\"\",\r\n            contact_number:\"\",\r\n            email:\"\"\r\n        }\r\n    }\r\n  },\r\n  methods:{\r\n    submit(){\r\n        this.$store.dispatch('staff/createClinic',this.clinic)\r\n    },\r\n    handleDialog(){\r\n      this.$store.state.staff.submit.dialog = false\r\n    }\r\n  }\r\n};\r\n</script>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}