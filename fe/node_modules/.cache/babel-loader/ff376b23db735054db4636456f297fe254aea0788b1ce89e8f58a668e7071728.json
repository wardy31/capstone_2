.personnelData = data;\r\n    },\r\n    closeDeleteDialog() {\r\n      this.deleteDialog = false;\r\n    },\r\n    deleteStaff() {\r\n      this.$store.dispatch(\"staff/delete\", this.personnelData);\r\n      this.deleteDialog = false;\r\n    },\r\n    handleDialog() {\r\n      console.log(\"wdd\");\r\n      this.$store.state.staff.submit.dialog = true;\r\n    },\r\n    handleRole(data) {\r\n      console.log(\"data\", data);\r\n      this.roleData = data;\r\n      this.$store.state.staff.role.dialog = true;\r\n    },\r\n    reset() {\r\n      this.roleData = null;\r\n    },\r\n    pageSwitch(data) {\r\n      this.page_number = data;\r\n      this.$store.dispatch(\"staff/staffs\", data);\r\n    },\r\n    handleSearch() {\r\n      this.$store.dispatch(\"staff/search\", this.inputSearch);\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.bg {\r\n  background-color: #f8f0f0;\r\n}\r\n.adj {\r\n  width: 280px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}